{
    "name":"YT Search",
    "version":"1.0",
    "manifest_version":2,
    "description":"Makes YT like a web browser.",
    "icons":{
        "16":"images/icon16.png",
        "48":"images/icon48.png",
        "128":"images/icon128.png"
    },
    "page_action":{
        "default_icon":{
            "16": "/images/icon16-2.png",
            "48":"images/icon48-2.png",
            "128":"images/icon128-2.png"
        }
    },
    "content_scripts":[
        {
            "matches":["https://www.youtube.com/"],
            "js":["content.js"]
        }
    ]
}